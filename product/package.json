{
  "scripts": {
    "build": "tsc --project main-src/tsconfig.json && ng build --configuration production",
    "start": "tsc --project main-src/tsconfig.json && concurrently \"cross-env NODE_ENV=develop electron ./dist-config\" \"ng build --watch\"",
    "_precommit": "lint-staged",
    "lint": "ng lint",
    "format": "ng lint --fix && prettier */*.{ts,scss} --write",
    "release-darwin": "npm run prebuild-darwin && npm run package-darwin",
    "release-win32": "npm run prebuild-win32 && npm run package-win32",
    "release-windows-store": "npm run prebuild-win32 && npm run package-windows-store",
    "prebuild-darwin": "npm run build && node build-scripts/for-mac-prebuild.js",
    "package-darwin": "export DEBUG=electron-osx-sign && node build-scripts/for-mac-packaging.js",
    "prebuild-win32": "npm run build && node build-scripts/for-windows-prebuild.js",
    "package-win32": "node build-scripts/for-windows-packaging.js",
    "package-windows-store": "node build-scripts/for-windows-store-packaging.js",
    "copy-binary-asssets": "node build-scripts/copy-binary-assets.js"
  },
  "lint-staged": {
    "*.ts": [
      "ng lint",
      "git add"
    ],
    "*.{js,json,css,md}": [
      "prettier",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@types/createjs": "^0.0.29",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.12",
    "crypto-js": "^3.3.0",
    "electron": "^15.3.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.15.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular-devkit/core": "12.2.7",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "12.2.7",
    "@angular/compiler-cli": "12.2.7",
    "@angular/language-service": "12.2.7",
    "@types/jquery": "^3.5.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "chokidar": "^3.5.2",
    "concurrently": "^4.1.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.1",
    "del": "^4.1.1",
    "electron-osx-sign": "0.4.11",
    "electron-packager": "^13.1.1",
    "electron-windows-store": "^2.1.0",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-react": "latest",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "mkdirp": "^0.5.5",
    "prettier": "1.18.2",
    "rxjs-compat": "^6.6.7",
    "ts-node": "^8.10.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "4.5.4"
  }
}